import java.util.Scanner;

class Vote {
public static void main(String args[]) {
Scanner sc = new Scanner(System.in);
System.out.println("Enter your table number:");
int number = sc.nextInt();
for (int i = 1; i <= 12; i++) {
System.out.println(number + " * " + i + " = " + (number * i));
        }
    }
}
